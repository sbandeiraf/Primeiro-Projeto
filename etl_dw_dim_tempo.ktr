    <sql>select  i::date as data
	, (
		(date_part('week', ((date_trunc('month',i) + interval '1 month') - interval '1 day')::date) - date_part('week', date_trunc('month',i)))+1
		- (date_part('week', ((date_trunc('month',i) + interval '1 month') - interval '1 day')::date) - date_part('week', i))
		)::int
		 as semana_mes
from generate_series(
					(extract(year from to_date('1980-01-02','YYYY-MM-DD'))||'-01-02')::date
